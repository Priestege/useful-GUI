local ScreenGui = Instance.new("ScreenGui") local Frame = Instance.new("Frame") local fly = Instance.new("TextButton") local RedBlue = Instance.new("TextButton") local InfinityYield = Instance.new("TextButton") local HoldDuration = Instance.new("TextButton") local Dexexplorer = Instance.new("TextButton") local ShiftlockAlt = 
Instance.new("TextButton") local hideall = 
Instance.new("TextButton") local VehicleFly = Instance.new("TextButton") local ExitButton = Instance.new("TextButton") local UserInputService = game:GetService("UserInputService") local dragging local dragInput local dragStart local startPos local function update(input) local delta = input.Position - dragStart Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y) end Frame.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragging = true dragStart = input.Position startPos = Frame.Position input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end) end end) Frame.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end end) UserInputService.InputChanged:Connect(function(input) if input == dragInput and dragging then update(input) end end) ScreenGui.Parent = game.CoreGui Frame.Parent = ScreenGui Frame.BackgroundColor3 = Color3.new( -0.7, -0.7, -0.3) Frame.Position = UDim2.new(0.05, 0, 0.10, 0) Frame.Size = UDim2.new(0, 200, 0, 300) fly.Parent = Frame fly.BackgroundColor3 = Color3.new(1, 1, 1) fly.Position = UDim2.new(0.05, 0, 0.1, 0) fly.Size = UDim2.new(0.9, 0, 0.10, 0) fly.Text = "FLY" fly.MouseButton1Click:Connect(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/MaksBloxX/NoClipScript/main/Fly%20V3", true))() end) RedBlue.Parent = Frame RedBlue.BackgroundColor3 = Color3.new(1, 1, 1) RedBlue.Position = UDim2.new(0.05, 0, 0.2, 0) RedBlue.Size = UDim2.new(0.9, 0, 0.10, 0) RedBlue.Text = "RED BLUE" RedBlue.MouseButton1Click:Connect(function() loadstring(game:HttpGet("https://raw.githubusercontent.com/JustAP1ayer/PlayerHubOther/main/PlayerHubIngameUgcLimiteds.lua"))() end) InfinityYield.Parent = Frame InfinityYield.BackgroundColor3 = Color3.new(1, 1, 1) InfinityYield.Position = UDim2.new(0.05, 0, 0.3, 0) InfinityYield.Size = UDim2.new(0.9, 0, 0.10, 0) InfinityYield.Text = "INFINITY YIELD" InfinityYield.MouseButton1Click:Connect(function() loadstring(game:HttpGet(('https://pastebin.com/raw/rShQCVmL')))() end) HoldDuration.Parent = Frame HoldDuration.BackgroundColor3 = Color3.new(1, 1, 1) HoldDuration.Position = UDim2.new(0.05, 0, 0.4, 0) HoldDuration.Size = UDim2.new(0.9, 0, 0.10, 0) HoldDuration.Text = "NO E WAIT" HoldDuration.MouseButton1Click:Connect(function() for _, v in pairs(workspace:GetDescendants()) do if v:IsA("ProximityPrompt") then v.HoldDuration = 0 task.wait() end end end) Dexexplorer.Parent = Frame Dexexplorer.BackgroundColor3 = Color3.new(1, 1, 1) Dexexplorer.Position = UDim2.new(0.05, 0, 0.5, 0) Dexexplorer.Size = UDim2.new(0.9, 0, 0.10, 0) Dexexplorer.Text = "DEX MOBILE" Dexexplorer.MouseButton1Click:Connect(function() getgenv().Key = "Bash" loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/MC%3AIY%20Dex",true))(); end) ShiftlockAlt.Parent = Frame ShiftlockAlt.BackgroundColor3 = Color3.new(1, 1, 1) ShiftlockAlt.Position = UDim2.new(0.05, 0, 0.6, 0) ShiftlockAlt.Size = UDim2.new(0.9, 0, 0.10, 0) ShiftlockAlt.Text = "SHIFT LOCK" ShiftlockAlt.MouseButton1Click:Connect(function() loadstring(game:HttpGet('https://raw.githubusercontent.com/Unknownproootest/Permanent-Shift-Lock-Alt/alt/PermShiftlockAlt'))(); end) hideall.Parent = Frame hideall.BackgroundColor3 = Color3.new(1, 1, 1) hideall.Position = UDim2.new(0.05, 0, 0.7, 0) hideall.Size = UDim2.new(0.9, 0, 0.10, 0) hideall.Text = "HIDE ALL" hideall.MouseButton1Click:Connect(function() _G.HideAll = true while _G.HideAll and task.wait() do for i,v in pairs(game.Players:GetPlayers()) do if v.Name ~= game.Players.LocalPlayer.Name and v.Character then v.Character:Destroy() end end end end) VehicleFly.Parent = Frame VehicleFly.BackgroundColor3 = Color3.new(1, 1, 1) VehicleFly.Position = UDim2.new(0.05, 0, 0.8, 0) VehicleFly.Size = UDim2.new(0.9, 0, 0.10, 0) VehicleFly.Text = "VEHICLE FLY" VehicleFly.MouseButton1Click:Connect(function() loadstring(game:HttpGet("https://pastebin.com/raw/MHE1cbWF"))() end) ExitButton.Parent = Frame ExitButton.BackgroundColor3 = Color3.new(3, 2, 1) ExitButton.Position = UDim2.new(0.05, 0, 0.9, 0) ExitButton.Size = UDim2.new(0.9, 0, 0.10, 0) ExitButton.Text = "EXIT" ExitButton.MouseButton1Click:Connect(function() ScreenGui:Destroy() end)
